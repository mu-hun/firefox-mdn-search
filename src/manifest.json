{
  "manifest_version": 2,
  "name": "MDN Search Web Extension",
  "version": "0.0.1",
  "description": "To use, type 'mdn' plus your search term into the url bar.",
  "icons": {
    "64": "icons/mdn64.png"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "MDN Search",
      "search_url": "https://mdn.io/{searchTerms}",
      "favicon_url": "https://developer.mozilla.org/static/img/opengraph-logo.72382e605ce3.png",
      "keyword": "mdn"
    }
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{fdc56311-fb5c-4e51-b46a-3d9796e11d28}",
      "strict_min_version": "55.0"
    }
  }
}
